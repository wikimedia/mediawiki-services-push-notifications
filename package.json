{
  "name": "push-notifications",
  "version": "0.0.1",
  "description": "A service for sending push notification messages",
  "main": "./dist/app.js",
  "scripts": {
    "start": "npm run serve",
    "serve": "service-runner",
    "build": "npm run build-ts && npm run lint-check",
    "build-ts": "tsc",
    "watch-ts": "tsc -w",
    "debug": "npm run build && npm run watch-debug",
    "clean": "rm -r dist",
    "test": "PREQ_CONNECT_TIMEOUT=15 mocha -r ts-node/register test/**/*.ts",
    "test:spec": "mocha -r ts-node/register test/features/app/spec.ts",
    "coverage": "PREQ_CONNECT_TIMEOUT=15 nyc --reporter=lcov _mocha -r ts-node/register test/**/*.ts",
    "watch-test": "npm run test -- --watchAll",
    "lint": "tsc --noEmit && npm run lint-check",
    "lint-check": "eslint \"**/*.{js,ts}\"",
    "lint-fix": "tsc --noEmit && eslint \"**/*.{js,ts}\" --quiet --fix",
    "watch": "concurrently -k -p \"[{name}]\" -n \"TypeScript,Node\" -c \"cyan.bold,green.bold\" \"npm run watch-ts\" \"npm run watch-node\"",
    "watch-debug": "concurrently -k -p \"[{name}]\" -n \"TypeScript,Node\" -c \"cyan.bold,green.bold\" \"npm run watch-ts\" \"npm run serve-debug\"",
    "watch-node": "nodemon dist/server.js --signal SIGTERM",
    "serve-debug": "nodemon --inspect dist/server.js --signal SIGTERM",
    "docker-start": "service-runner docker-start",
    "docker-test": "service-runner docker-test",
    "test-build": "service-runner docker-test && service-runner build --deploy-repo --force"
  },
  "repository": {
    "type": "git",
    "url": "git://github.com/wikimedia/mediawiki-services-push-notifications.git"
  },
  "keywords": [
    "REST",
    "API",
    "push",
    "notification",
    "FCM",
    "APNs"
  ],
  "author": "Wikimedia Service Team <services@lists.wikimedia.org>",
  "contributors": [],
  "license": "Apache-2.0",
  "bugs": {
    "url": "https://phabricator.wikimedia.org/tag/Push-Notification-Service/"
  },
  "homepage": "https://www.mediawiki.org/wiki/Wikimedia_Product_Infrastructure_team/Push_Notifications_Infrastructure",
  "dependencies": {
    "@parse/node-apn": "^3.1.0",
    "bluebird": "^3.5.5",
    "body-parser": "^1.19.0",
    "bunyan": "^1.8.12",
    "compression": "^1.7.4",
    "domino": "^2.1.3",
    "express": "^4.17.1",
    "express-rate-limit": "^5.1.3",
    "firebase-admin": "^8.12.1",
    "http-shutdown": "^1.2.1",
    "js-yaml": "^3.13.1",
    "preq": "^0.5.9",
    "service-runner": "^2.7.1",
    "swagger-router": "^0.7.4",
    "swagger-ui-dist": "^3.22.3",
    "uuid": "^3.3.2"
  },
  "devDependencies": {
    "@types/bluebird": "^3.5.30",
    "@types/body-parser": "^1.19.0",
    "@types/compression": "^1.7.0",
    "@types/concurrently": "^5.2.0",
    "@types/eslint": "^6.8.0",
    "@types/express": "^4.17.6",
    "@types/js-yaml": "^3.12.3",
    "@types/mocha": "^7.0.2",
    "@types/node": "^13.13.4",
    "@types/shelljs": "^0.8.7",
    "@typescript-eslint/eslint-plugin": "^2.31.0",
    "@typescript-eslint/parser": "^2.31.0",
    "ajv": "^6.5.4",
    "concurrently": "^5.2.0",
    "eslint-config-wikimedia": "^0.10.0",
    "eslint-plugin-jsdoc": "^4.0.1",
    "eslint-plugin-json": "^1.2.1",
    "extend": "^3.0.2",
    "mocha": "^5.2.0",
    "mocha-lcov-reporter": "^1.3.0",
    "mocha.parallel": "^0.15.6",
    "nodemon": "^2.0.3",
    "nyc": "^14.1.1",
    "openapi-schema-validator": "^3.0.3",
    "ts-node": "^8.10.1",
    "typescript": "^3.8.3"
  },
  "deploy": {
    "target": "debian",
    "node": "10.20.1",
    "dependencies": {
      "_all": []
    }
  }
}
